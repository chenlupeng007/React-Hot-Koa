{
  "name": "react-koa-fullstack",
  "version": "1.0.0",
  "scripts": {
    "start": "NODE_ENV=development nodemon src/server --watch src/server --exec babel-node",
    "start-prod": "yarn clean && yarn build && NODE_ENV=production babel-node src/server",
